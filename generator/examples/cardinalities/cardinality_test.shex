prefix : <http://example.org/>
prefix xsd: <http://www.w3.org/2001/XMLSchema#>

:Person {
  :name xsd:string ;
  :birthdate xsd:date ? ;
  :enrolledIn @:Course * ;
  :teaches @:Course ;
  :advises @:Course + ;
  :memberOf @:Group ? ;
  :hasBadge @:Badge {2,4}
}
:Course {
  :name xsd:string
}
:Group {
  :groupName xsd:string
}
:Badge {
  :badgeName xsd:string
}
